{
  "dashboard": {
    "title": "Multi-Agent System Performance",
    "uid": "multi-agent-performance",
    "version": 1,
    "timezone": "browser",
    "tags": ["multi-agent", "performance", "monitoring"],
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1},
        "type": "row",
        "title": "System Resources",
        "collapsed": false
      },
      {
        "id": 2,
        "gridPos": {"x": 0, "y": 1, "w": 8, "h": 8},
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "multi_agent_system_cpu_percent",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "CPU Usage",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "thresholds": [
          {
            "value": 85,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ],
        "alert": {
          "name": "High CPU Usage",
          "conditions": [
            {
              "evaluator": {
                "params": [85],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "handler": 1,
          "message": "CPU usage is above 85%",
          "noDataState": "no_data"
        }
      },
      {
        "id": 3,
        "gridPos": {"x": 8, "y": 1, "w": 8, "h": 8},
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "multi_agent_system_memory_percent",
            "legendFormat": "Memory %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Memory Usage",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "thresholds": [
          {
            "value": 85,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {"x": 16, "y": 1, "w": 8, "h": 8},
        "type": "graph",
        "title": "Disk I/O",
        "targets": [
          {
            "expr": "rate(multi_agent_system_disk_io_read_bytes[5m])",
            "legendFormat": "Read",
            "refId": "A"
          },
          {
            "expr": "rate(multi_agent_system_disk_io_write_bytes[5m])",
            "legendFormat": "Write",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "Bps",
            "label": "Bytes/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "gridPos": {"x": 0, "y": 9, "w": 12, "h": 8},
        "type": "graph",
        "title": "Network Traffic",
        "targets": [
          {
            "expr": "sum(rate(multi_agent_system_network_bytes_sent[5m]))",
            "legendFormat": "Sent",
            "refId": "A"
          },
          {
            "expr": "sum(rate(multi_agent_system_network_bytes_recv[5m]))",
            "legendFormat": "Received",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "Bps",
            "label": "Bytes/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"x": 12, "y": 9, "w": 12, "h": 8},
        "type": "graph",
        "title": "Network Errors",
        "targets": [
          {
            "expr": "sum(rate(multi_agent_system_network_errors_in[5m]))",
            "legendFormat": "Errors In",
            "refId": "A"
          },
          {
            "expr": "sum(rate(multi_agent_system_network_errors_out[5m]))",
            "legendFormat": "Errors Out",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Errors/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"x": 0, "y": 17, "w": 24, "h": 1},
        "type": "row",
        "title": "Agent Performance",
        "collapsed": false
      },
      {
        "id": 8,
        "gridPos": {"x": 0, "y": 18, "w": 12, "h": 8},
        "type": "graph",
        "title": "Agent Execution Times (Mean)",
        "targets": [
          {
            "expr": "rate(multi_agent_system_agent_execution_time_sum[5m]) / rate(multi_agent_system_agent_execution_time_count[5m])",
            "legendFormat": "{{agent_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 9,
        "gridPos": {"x": 12, "y": 18, "w": 12, "h": 8},
        "type": "graph",
        "title": "Agent Execution Count",
        "targets": [
          {
            "expr": "rate(multi_agent_system_agent_execution_count[5m])",
            "legendFormat": "{{agent_name}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Executions/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "gridPos": {"x": 0, "y": 26, "w": 12, "h": 8},
        "type": "graph",
        "title": "Agent Errors",
        "targets": [
          {
            "expr": "rate(multi_agent_system_agent_error_count[5m])",
            "legendFormat": "{{agent_name}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Errors/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 11,
        "gridPos": {"x": 12, "y": 26, "w": 12, "h": 8},
        "type": "stat",
        "title": "Total Agent Executions",
        "targets": [
          {
            "expr": "sum(multi_agent_system_agent_execution_count)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "auto"
        }
      },
      {
        "id": 12,
        "gridPos": {"x": 0, "y": 34, "w": 24, "h": 1},
        "type": "row",
        "title": "API Performance",
        "collapsed": false
      },
      {
        "id": 13,
