{
  "name": "hcmc-traffic-backend",
  "version": "1.0.0",
  "description": "HCMC Traffic Monitoring Backend API",
  "main": "dist/server.js",
  "scripts": {
    "setup": "node scripts/setup/setup.js",
    "test:connections": "node tests/integration/test-connections.js",
    "test:camera": "node tests/integration/test-camera-endpoint.js",
    "test:weather": "node tests/integration/test-weather-endpoint.js",
    "test:airquality": "node tests/integration/test-airquality-endpoint.js",
    "test:all": "node tests/integration/test-all-endpoints.js",
    "test:agents": "ts-node tests/integration/test-enhanced-agents.ts",
    "health:check": "pwsh scripts/health-checks/check-health.ps1",
    "health:data": "pwsh scripts/health-checks/check-real-data.ps1",
    "verify": "node scripts/health-checks/verify-implementation.js",
    "start": "node --max-old-space-size=4096 dist/server.js",
    "dev": "set NODE_OPTIONS=--max-old-space-size=4096 && npm run setup && ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.ts$",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "traffic",
    "monitoring",
    "hcmc",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@turf/turf": "^7.2.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "neo4j-driver": "^5.14.0",
    "node-cache": "^5.1.2",
    "openai": "^6.9.1",
    "pg": "^8.11.3",
    "sparql-http-client": "^2.4.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.0",
    "@types/node-cache": "^4.1.3",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "license-checker": "^25.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
